{
	"name": "svelte-dnd-list",
	"version": "0.1.8",
	"description": "Simple and lightweight Svelte drag and drop library",
	"keywords": [
		"svelte",
		"drag",
		"drop",
		"list",
		"sortable",
		"component",
		"dnd",
		"small"
	],
	"homepage": "https://github.com/tarb/svelte-dnd-list",
	"bugs": {
		"url": "https://github.com/tarb/svelte-dnd-list/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tarb/svelte-dnd-list.git"
	},
	"author": "Rob Tarbath",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-package package",
		"publish": "svelte-package publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.4",
		"@sveltejs/kit": "1.1.1",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.57.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.2",
		"svelte-preprocess": "^5.0.0",
		"@sveltejs/package": "1.0.2",
		"svelte2tsx": "^0.6.0",
		"tslib": "^2.4.1",
		"typescript": "~4.9.4"
	},
	"type": "module"
}
